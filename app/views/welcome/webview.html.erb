
  <h4 class="title"><i class="fa fa-credit-card-alt" aria-hidden="true"></i> Transactions web details</h4>
  <br>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Titre</th>
        <th scope="col">Details</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <th scope="row" class="text-left">Action</th>
        <td><%= @data.flag%></td>
      </tr>
      <tr>
        <th scope="row">Client (vous)</th>
        <td><%= Customer.find(@data.customer_id).complete_name %></td>
      </tr>
      <tr>
        <th scope="row">Montant</th>
        <td><%= @data.amount.round(2)%></td>
      </tr>
      <tr>
        <th scope="row">Code</th>
        <td><%= @data.code%></td>
      </tr>
      <tr>
        <th scope="row">Date</th>
        <td><%= @data.created_at %></td>
      </tr>
      <tr>
          <th>RÃ©gion</th>
          <td>
              <%= @data.region %>
          </td>
      </tr>
    </tbody>
  </table>
  <br>
  <%= map(:center => {
    :latlng => [@data.lat, @data.long],
    :zoom => 10
  },
  :markers => [
     {
       :latlng => [@data.lat, @data.long],
       :popup => "Lieu Transaction"
     }
  ]) %>
  <blockquote class="blockquote mt-5">
    <p class="mb-0">
      <footer class="blockquote-footer">Creer un compte/se connecter</footer>
      <div class="row mt-2">
        <div class=" col-6">
          <a href="#" class="btn btn-primary btn-lg btn-block">Se connecter</a>
        </div>
        <div class=" col-6">
          <a href="#" class="btn btn-primary btn-lg btn-block">Creer</a>
        </div>
      </div>
      <hr>
      <footer class="blockquote-footer">Ou se connecter avec</footer>
      <div class="row mt-2">
        <div class=" col-4">
          <%= link_to "#" do %>
            <%= image_tag("fb.png", size: 50, class: "rounded mx-auto d-block") %>
          <% end %>
        </div>
        <div class=" col-4">
          <%= link_to "#" do %>
            <%= image_tag("google.png", size: 50, class: "rounded mx-auto d-block") %>
          <% end %>
        </div>
        <div class=" col-4">
          <%= link_to "#" do %>
            <%= image_tag("twitter.png", size: 50, class: "rounded mx-auto d-block") %>
          <% end %>
        </div>
      </div>
    </p>
    <footer class="blockquote-footer">Someone famous in <cite title="Source Title">Source Title</cite></footer>
  </blockquote>